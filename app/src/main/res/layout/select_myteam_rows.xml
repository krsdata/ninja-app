<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:background="@color/background_default"
    android:layout_marginLeft="15dip"
    android:layout_marginRight="15dip"
    android:layout_marginTop="@dimen/_6sdp"
    android:layout_marginBottom="@dimen/_6sdp"
    android:orientation="vertical">
   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:paddingLeft="@dimen/_8sdp"
       android:orientation="horizontal"
       android:layout_marginBottom="@dimen/_2sdp"
       >
       <TextView
           android:id="@+id/user_team_name"
           android:layout_width="0dp"
           android:layout_weight="1"
           android:layout_height="match_parent"
           android:text="TEAMA(T1)"
           android:textColor="@color/text"
           android:gravity="center_vertical"
           android:textSize="12sp"
           android:fontFamily="@font/montserrat_bold"/>

      <LinearLayout
          android:layout_width="@dimen/_16sdp"
          android:layout_height="@dimen/_16sdp"
          android:gravity="center_vertical|right"
          android:background="@drawable/circle_red"
          android:orientation="horizontal">

         <ImageView
             android:id="@+id/team_select"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             android:padding="@dimen/_2sdp"
             android:src="@drawable/checked" />
      </LinearLayout>
   </LinearLayout>

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
      android:orientation="vertical">

      <RelativeLayout
          android:layout_width="wrap_content"
          android:layout_height="wrap_content">

         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="@dimen/_60sdp"
             android:background="@color/edit_text"
             android:backgroundTint="@color/fab"
             android:layout_marginTop="@dimen/_16sdp"
             android:paddingLeft="@dimen/_8sdp"
             android:paddingRight="@dimen/_8sdp"
             android:gravity="center_vertical"
             android:orientation="horizontal"
             >
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:paddingLeft="@dimen/_8sdp"
                android:paddingRight="@dimen/_8sdp"
                android:layout_marginBottom="@dimen/_6sdp"
                android:orientation="horizontal"
                >
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="WK"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:textSize="14sp"
                   android:fontFamily="@font/montserrat_medium"/>
               <TextView
                   android:id="@+id/count_wicketkeeper"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="2"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:layout_marginLeft="@dimen/_8sdp"
                   android:textSize="14sp"
                   android:fontFamily="@font/montserrat_bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/_8sdp"
                android:layout_gravity="bottom"
                android:layout_marginBottom="@dimen/_6sdp"
                android:paddingRight="@dimen/_8sdp"
                android:orientation="horizontal"
                >
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="BAT"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:textSize="12sp"
                   android:fontFamily="@font/montserrat_medium"/>
               <TextView
                   android:id="@+id/count_batsman"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="6"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:layout_marginLeft="@dimen/_8sdp"
                   android:textSize="14sp"
                   android:fontFamily="@font/montserrat_bold"/>

            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/_8sdp"
                android:layout_gravity="bottom"
                android:layout_marginBottom="@dimen/_6sdp"
                android:paddingRight="@dimen/_8sdp"
                android:orientation="horizontal"
                >
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="ALL"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:textSize="12sp"
                   android:fontFamily="@font/montserrat_medium"/>
               <TextView
                   android:id="@+id/count_allrounder"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="4"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:layout_marginLeft="@dimen/_8sdp"
                   android:textSize="14sp"
                   android:fontFamily="@font/montserrat_bold"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/_8sdp"
                android:layout_gravity="bottom"
                android:layout_marginBottom="@dimen/_6sdp"
                android:paddingRight="@dimen/_8sdp"
                android:orientation="horizontal"
                >
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="BOWL"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:textSize="12sp"
                   android:fontFamily="@font/montserrat_medium"/>
               <TextView
                   android:id="@+id/count_bowler"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="4"
                   android:textColor="@color/white"
                   android:layout_gravity="center"
                   android:layout_marginLeft="@dimen/_8sdp"
                   android:textSize="14sp"
                   android:fontFamily="@font/montserrat_bold"/>

            </LinearLayout>

         </LinearLayout>

         <LinearLayout
             android:layout_width="match_parent"
             android:layout_height="@dimen/_50sdp"
             android:paddingLeft="@dimen/_8sdp"
             android:background="@drawable/card_rounded"
             android:paddingRight="@dimen/_8sdp"
             android:gravity="center_vertical"
             android:backgroundTint="@color/white"
             android:orientation="horizontal"
             >


            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical|right"
                android:padding="10dip"
                >
               <TextView
                   android:id="@+id/teama_name"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="SL-W"
                   android:maxLength="4"
                   android:maxLines="1"
                   android:textColor="@color/text"
                   android:layout_gravity="center"
                   android:textSize="12sp"
                   android:fontFamily="@font/montserrat_bold"/>
               <TextView
                   android:id="@+id/teama_count"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="5"
                   android:textColor="@color/color_card"
                   android:layout_gravity="center"
                   android:textSize="16sp"
                   android:fontFamily="@font/montserrat_bold"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical|right"
                android:padding="10dip"
                >
               <TextView
                   android:id="@+id/teamb_name"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="BD-W"
                   android:maxLength="4"
                   android:maxLines="1"
                   android:textColor="@color/text"
                   android:layout_gravity="center"
                   android:textSize="12sp"
                   android:fontFamily="@font/montserrat_bold"/>
               <TextView
                   android:id="@+id/teamb_count"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:text="6"
                   android:textColor="@color/color_card"
                   android:layout_gravity="center"
                   android:textSize="16sp"
                   android:fontFamily="@font/montserrat_bold"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical|right"
                android:padding="10dip"
                >
               <RelativeLayout
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:gravity="center"
                   android:orientation="vertical"
                   >

                  <LinearLayout
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:gravity="center"
                      android:orientation="vertical"
                      android:layout_marginLeft="2dip">

                     <ImageView
                         android:id="@+id/trump_imageView"
                         android:layout_width="@dimen/_25sdp"
                         android:layout_height="@dimen/_25sdp"
                         android:src="@drawable/placeholder_player_teamb"
                         />
                     <TextView
                         android:id="@+id/trump_player_name"
                         android:layout_width="wrap_content"
                         android:layout_height="15dp"
                         android:text="Virat Kohli"
                         android:singleLine="true"
                         android:maxLines="1"
                         android:paddingLeft="3dp"
                         android:paddingRight="3dp"
                         android:textColor="@color/text"
                         android:textSize="8sp"/>

                  </LinearLayout>
                  <TextView
                      android:layout_width="@dimen/_18sdp"
                      android:layout_height="@dimen/_18sdp"
                      android:text="T"
                      android:gravity="center"
                      android:layout_centerHorizontal="true"
                      android:layout_centerVertical="true"
                      android:background="@drawable/bg_text"
                      android:fontFamily="@font/montserrat_medium"
                      android:backgroundTint="@color/color_card"
                      style="@style/ground_style_players_info"
                      android:textSize="12sp"
                      android:textColor="@color/white"/>

               </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical|right"
                android:padding="10dip"
                >
               <RelativeLayout
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:gravity="center"
                   android:orientation="vertical"
                   >

                  <LinearLayout
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:gravity="center"
                      android:orientation="vertical"
                      android:layout_marginLeft="2dip">

                     <ImageView
                         android:id="@+id/captain_imageView"
                         android:layout_width="@dimen/_25sdp"
                         android:layout_height="@dimen/_25sdp"
                         android:src="@drawable/placeholder_player_teama"
                         />
                     <TextView
                         android:id="@+id/captain_player_name"
                         android:layout_width="wrap_content"
                         android:layout_height="15dp"
                         android:text="Virat Kohli"
                         android:singleLine="true"
                         android:maxLines="1"
                         android:paddingLeft="3dp"
                         android:paddingRight="3dp"
                         android:textColor="@color/text"
                         android:textSize="8sp"/>

                  </LinearLayout>
                  <TextView
                      android:layout_width="@dimen/_18sdp"
                      android:layout_height="@dimen/_18sdp"
                      android:text="C"
                      android:gravity="center"
                      android:fontFamily="@font/montserrat_medium"
                      android:backgroundTint="@color/color_card"
                      android:background="@drawable/circle_black"
                      android:layout_centerHorizontal="true"
                      android:layout_centerVertical="true"
                      style="@style/ground_style_players_info"
                      android:textSize="12sp"
                      android:textColor="@color/white"/>

               </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:gravity="center_vertical|right"
                android:padding="10dip"
                >
               <RelativeLayout
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:gravity="center"
                   android:orientation="vertical"
                   >

                  <LinearLayout
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:gravity="center"
                      android:orientation="vertical"
                      android:layout_marginLeft="2dip">

                     <ImageView
                         android:id="@+id/vc_imageView"
                         android:layout_width="@dimen/_25sdp"
                         android:layout_height="@dimen/_25sdp"
                         android:src="@drawable/placeholder_player_teamb"
                         />
                     <TextView
                         android:id="@+id/vc_player_name"
                         android:layout_width="wrap_content"
                         android:layout_height="15dp"
                         android:text="Virat Kohli"
                         android:singleLine="true"
                         android:maxLines="1"
                         android:paddingLeft="3dp"
                         android:paddingRight="3dp"
                         android:textColor="@color/text"
                         android:textSize="8sp"/>

                  </LinearLayout>
                  <TextView
                      android:layout_width="@dimen/_18sdp"
                      android:layout_height="@dimen/_18sdp"
                      android:text="V"
                      android:backgroundTint="@color/color_card"
                      android:layout_centerHorizontal="true"
                      android:layout_centerVertical="true"
                      android:background="@drawable/circle_black"
                      android:fontFamily="@font/montserrat_medium"
                      style="@style/ground_style_players_info"
                      android:textSize="12sp"
                      android:textColor="@color/white"/>

               </RelativeLayout>

            </LinearLayout>

         </LinearLayout>







      </RelativeLayout>
   </LinearLayout>



   <!--LAyer 2-->

</LinearLayout>